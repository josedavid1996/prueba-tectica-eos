{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "next"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
    "@next/next/no-document-import-in-page": "off"
  }
}
